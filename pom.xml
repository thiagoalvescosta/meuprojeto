<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<project>
    <modelVersion>4.0.0</modelVersion>
    <groupId>br.com.techne</groupId>
    <artifactId>my-app</artifactId>
    <packaging>war</packaging>
    <name>svn</name>
    <version>1.0-SNAPSHOT</version>
    <url>http://techne.com.br</url>
    <build>
        <sourceDirectory>src</sourceDirectory>
        <resources/>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>3.3</version>
                <configuration>
                    <source>1.8</source>
                    <target>1.8</target>
                </configuration>
            </plugin>
        </plugins>
    </build>
    <repositories>
        <repository>
            <id>artifactory-cronospaas</id>
            <name>artifactory-cronospaas-releases</name>
            <url>http://artifactory.cronospaas.com.br/artifactory/ext-release-local</url>
        </repository>
    </repositories>
    <dependencies/>
    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
        <project.cronospass.resource encoding="UTF-8" mimetype="folder/src">src</project.cronospass.resource>
    </properties>
</project>
